{
    "$schema": "https://json-schema.org/draft/2019-09/schema",
    "$id": "http://example.com/example.json",
    "type": "object",
    "default": {},
    "title": "New Category Schema",
    "required": [
        "user_id",
        "name",
        "budget_id",
        "allocation_id"
    ],
    "properties": {
        "user_id": {
            "type": "integer",
            "default": 0,
            "title": "The user_id Schema",
            "examples": [
                1
            ]
        },
        "name": {
            "type": "string",
            "default": "",
            "title": "The name Schema",
            "examples": [
                "new budget"
            ]
        },
        "budget_id": {
            "type": "integer",
            "default": "",
            "title": "The budget_id Schema",
            "examples": [
                1,2,3
            ]
        },
        "allocation_id": {
            "type": "integer",
            "default": "",
            "title": "The allocation_id Schema",
            "examples": [
                1,2,3
            ]
        }
    },
    "examples": [{
        "user_id": 1,
        "name": "new budget",
        "budget_id": 1,
        "allocation_id": 1
    }]
}