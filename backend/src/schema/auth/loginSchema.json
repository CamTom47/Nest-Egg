{
    "$schema": "https://json-schema.org/draft/2019-09/schema",
    "$id": "http://example.com/example.json",
    "type": "object",
    "default": {},
    "title": "New User Schema",
    "required": ["username", "password"],
    "properties": {
        "username": {
            "type": "string",
            "default": "",
            "title": "The username Schema",
            "examples": [
                "testUser"
            ]
        },
        "password": {
            "type": "string",
            "default": "",
            "title": "The password Schema",
            "examples": [
                "testPassword"
            ]
        },
    "examples": [{
        "first_name": "testF",
        "last_name": "testL",
        "username": "testUser",
        "password": "testPassword",
        "email": "test@test.com",
        "is_admin": true
    }]
}
}