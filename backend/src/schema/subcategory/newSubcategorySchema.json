{
    "$schema": "https://json-schema.org/draft/2019-09/schema",
    "$id": "http://example.com/example.json",
    "type": "object",
    "default": {},
    "title": "New Subcategory Schema",
    "required": [
        "user_id",
        "name",
        "description",
        "category_id",
        "system_default"
    ],
    "properties": {
        "user_id": {
            "type": "integer",
            "default": 0,
            "title": "The user_id Schema",
            "examples": [
                1
            ]
        },
        "name": {
            "type": "string",
            "default": "",
            "title": "The name Schema",
            "examples": [
                "new budget"
            ]
        },
        "description": {
            "type": "string",
            "default": "",
            "title": "The description Schema",
            "examples": [
                "new budget description"
            ]
        },
        "category_id": {
            "type": "integer",
            "default": 0,
            "title": "The category_id Schema",
            "examples": [
                1
            ]
        },
        "system_default": {
            "type": "boolean",
            "default": "",
            "title": "The system_default Schema",
            "examples": [
                true
            ]
        }
    },
    "examples": [{
        "user_id": 1,
        "name": "new budget",
        "description": "new budget description",
        "category_id": 1,
        "system_default": true
    }]
}